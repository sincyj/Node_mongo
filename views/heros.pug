extends layout

block content
	
	each val in data
		.row
			.col6
				.card-panel.teal.lighten-2.z-depth-5
					.card-content.white-text
						.card-title
							
							h4 #{val.name}
						p Phone number: #{val.PhoneNo}
													
					.card-action
						a(href='deleteRow?name='+val.name).btn Delete
						a(href='viewData?_id='+val._id).btn View